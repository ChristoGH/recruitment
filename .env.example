# OpenAI API Key
OPENAI_API_KEY=your_openai_api_key_here

# Neo4j Configuration (for local Neo4j Desktop)
# Use the connection details from your Neo4j Desktop instance
# Default URI for local Neo4j Desktop is typically:
NEO4J_URI=bolt://localhost:7687
# Or if using Neo4j Desktop with a specific database:
# NEO4J_URI=bolt://localhost:7687/database/your_database_name
NEO4J_USER=neo4j
NEO4J_PASSWORD=your_neo4j_password_here

# RabbitMQ Configuration (optional, defaults shown)
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USER=guest
RABBITMQ_PASSWORD=guest 